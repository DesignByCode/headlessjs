{
  "name": "vanilla-headless",
  "version": "0.6.4",
  "main": "build/index.js",
  "description": "Inspired by HeadlessUi, Vanilla Headless is Web Component library that adds interactive with keyboard and mouse events",
  "repository": "https://github.com/DesignByCode/vanilla-headless.git",
  "homepage": "https://github.com/DesignByCode/vanilla-headless/#readme",
  "bugs": {
    "url": "https://github.com/DesignByCode/vanilla-headless/issues"
  },
  "scripts": {
    "dev:ts": "tsc -p tsconfig.json --watch",
    "dev:mix": "npx mix watch",
    "dev": "run-p dev:*",
    "clean": "rd /s /q esm dist lib build .cache>nul 2>&1|echo.>nul",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "precommit": "npm run test && lint-staged",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.json && npx mix --production"
  },
  "keywords": [
    "headlessui",
    "popover",
    "dropdown",
    "navigation toggle"
  ],
  "author": "designbycode",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "gulp": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "laravel-mix": "^6.0.49",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
